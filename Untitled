<body>
  <input type="number" id="n1" >
  <input type="number" id="n2">
  <button type="button" name="addtion">+</button>
  <script>
  let a = n1;
  let b = n2;

  function add(){
    a + b
  }
  $("button").click((){
   add()
})
  </script>
